{
  "name": "ai-wrapped",
  "private": true,
  "scripts": {
    "dev": "vite build && electrobun dev --console",
    "dev:hmr": "concurrently \"vite --port 5173\" \"electrobun dev --console\"",
    "build": "vite build && electrobun build",
    "build:prod": "vite build && electrobun build --env=stable",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "clean": "rm -rf dist .electrobun"
  },
  "dependencies": {
    "electrobun": "latest",
    "react": "latest",
    "react-dom": "latest",
    "recharts": "latest"
  },
  "devDependencies": {
    "@tailwindcss/vite": "latest",
    "@types/bun": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@vitejs/plugin-react": "latest",
    "concurrently": "latest",
    "tailwindcss": "latest",
    "typescript": "latest",
    "vite": "latest"
  }
}
