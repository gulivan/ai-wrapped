  "keywords": ["ai", "wrapped", "claude", "codex", "gemini", "copilot", "stats", "dashboard"],
  "scripts": {
    "dev": "bun run build && electrobun dev --console",
    "dev:hmr": "bun run build && concurrently \"vite --port 5173\" \"electrobun dev --console\"",
