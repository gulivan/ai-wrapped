{
  "name": "ai-wrapped",
  "version": "0.0.1",
  "description": "Your year in AI â€” a Spotify Wrapped-style dashboard for AI coding agents",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gulivan/ai-wrapped.git"
  },
  "keywords": ["ai", "wrapped", "claude", "codex", "gemini", "copilot", "stats", "dashboard"],
  "bin": {
    "ai-wrapped": "bin/cli.ts"
  },
  "files": [
    "bin/",
    "src/bun/",
    "src/shared/",
    "dist/",
    "public/",
    "icon.iconset/",
    "icon.png",
    "icon.ico",
    "icon.icns",
    "electrobun.config.ts",
    "tsconfig.json",
    "README.md",
    "LICENSE"
